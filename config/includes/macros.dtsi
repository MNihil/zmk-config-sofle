/ {
    
    macros {
        mc01ps1: mc01ps1 {
            label = "ZM_mc01_psc01";
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            wait-ms = <40>;
            tap-ms = <50>;
            bindings
                = <&macro_press &kp LSHFT>
                , <&macro_tap &kp I>
                , <&macro_release &kp LSHFT>
                , <&macro_tap &kp C &kp A &kp N &kp D &kp O &kp T &kp H &kp I &kp S &kp N0 &kp EXCL &kp EXCL>
                ;
        };

        mc01ps2: mc01ps2 {
            label = "ZM_mc01_psc02";
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            wait-ms = <40>;
            tap-ms = <50>;
            bindings
                = <&macro_press &kp LSHFT>
                , <&macro_tap &kp I &kp C>
                , <&macro_release &kp LSHFT>
                , <&macro_tap &kp A &kp N &kp D &kp O &kp T &kp H &kp I &kp S &kp N0 &kp EXCL &kp EXCL>
                ;
        };

        mc01ps3: mc01ps3 {
            label = "ZM_mc01_psc03";
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            wait-ms = <40>;
            tap-ms = <50>;
            bindings
                = <&macro_press &kp LSHFT>
                , <&macro_tap &kp I &kp C>
                , <&macro_release &kp LSHFT>
                , <&macro_tap &kp A &kp N &kp D &kp O &kp N0 &kp EXCL &kp EXCL>
                ;
        };

        mc02sc01: mc02sc01 {
            label = "ZM_mc02_sc01";
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            wait-ms = <40>;
            tap-ms = <50>;
            bindings
                = <&macro_press &kp LALT>
                , <&macro_tap &kp F1>
                , <&macro_release &kp LALT>
                ;
        };

        mc02sc02: mc02sc02 {
            label = "ZM_mc02_sc01";
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            wait-ms = <40>;
            tap-ms = <50>;
            bindings
                = <&macro_press &kp LALT>
                , <&macro_tap &kp F2>
                , <&macro_release &kp LALT>
                ;
        };
    };
};